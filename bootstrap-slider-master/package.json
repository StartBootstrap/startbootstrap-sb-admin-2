    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-contrib-jasmine": "~0.5.2",
    "grunt-template": "~0.2.0",
    "grunt-open": "~0.2.2",
    "bower": "latest",
    "grunt-contrib-less": "~0.7.0",
